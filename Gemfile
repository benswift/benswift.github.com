  gem "jekyll-youtube"
  # nokogiri is required for the Revealify plugin
  gem "nokogiri"
  # for testing all the links work
  gem 'html-proofer'
  gem "jekyll-paginate"
  gem "jekyll-sitemap"
  gem "jekyll-assets", "~> 3.0"
  gem "jekyll-compose"
end
